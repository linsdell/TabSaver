{
  "manifest_version": 2,
  "name": "TabSaver",
  "version": "0.1",
  "permissions": ["tabs"],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js","content.js"]
    }
  ],
  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "UI.html"
  },
  "background": {
  "scripts": ["background.js"]
}

}
